<nav>
  <a class="home" href="/"><img class="icon" src="https://i.imgur.com/PgxrRFA.png?1" alt="home"></a>
  <a class="menu" href="/user"><img class="avatar" src="{{avatar}}" alt="avatar"></a>
  <a class="menu" href="/browse"><img class="icon" src="http://i.imgur.com/DtKBRC0.png?1" alt="browse"></a>
</nav>
<div class="main-div container">
  <h1 class="fadeIn page-title">{{ title }}</h1>
  <img class="prof-avatar" src="{{ info.avatar }}">
  <p id="profUserNameEdit"> {{ info.name }}  <a href="#" id="profEditLink"> edit</a></p>
  <form action="/user" class="profileEdit">
    <input id="nameReplace" name="newName" class="profileEdit" placeholder="update username">
    <button type="submit" class="profileEdit">submit</button>
  </form>
  <p>gamesWon: {{ info.score.gamesWon }}</p>
  <p>gamesPlayed: {{ info.score.gamesPlayed }}</p>
  <a href="/auth/logout">logout</a>
</div>

